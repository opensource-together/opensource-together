{
  "name": "backend",
  "version": "0.2.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:backend:update": "docker compose --project-directory .. exec backend pnpm install",
    "prisma:generate": "pnpm prisma generate --schema=src/persistence/orm/prisma/schema.prisma && docker compose --project-directory .. exec backend pnpm prisma generate --schema=/app/src/persistence/orm/prisma/schema.prisma",
    "prisma:migrate:dev": "docker compose --project-directory .. exec backend pnpm prisma migrate dev --schema=/app/src/persistence/orm/prisma/schema.prisma && pnpm prisma generate --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:migrate:deploy": "docker compose --project-directory .. exec backend pnpm prisma migrate deploy --schema=/app/src/persistence/orm/prisma/schema.prisma",
    "prisma:migrate:reset": "docker compose --project-directory .. exec backend pnpm prisma migrate reset --schema=/app/src/persistence/orm/prisma/schema.prisma",
    "prisma:studio": "pnpm prisma studio --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:reset:local": "pnpm prisma migrate reset --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:generate:local": "pnpm prisma generate --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:migrate:dev:local": "pnpm prisma migrate dev --schema=src/persistence/orm/prisma/schema.prisma && pnpm prisma generate --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:push:local": "pnpm prisma db push --schema=src/persistence/orm/prisma/schema.prisma && pnpm prisma generate --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:migrate:deploy:local": "pnpm prisma migrate deploy --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:migrate:reset:local": "pnpm prisma migrate reset --schema=src/persistence/orm/prisma/schema.prisma",
    "prisma:studio:local": "pnpm prisma studio --schema=src/persistence/orm/prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/s3-request-presigner": "^3.850.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@octokit/auth-oauth-app": "^9.0.1",
    "@octokit/core": "^7.0.3",
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "6.12.0",
    "@sentry/nestjs": "^9.41.0",
    "@sentry/profiling-node": "^9.41.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "octokit": "^5.0.3",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.7.0",
    "rxjs": "^7.8.2",
    "supertokens-nestjs": "^0.0.2",
    "supertokens-node": "^22.1.1",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.16.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prisma": "6.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@application/(.*)$": "<rootDir>/application/$1",
      "^@domain/(.*)$": "<rootDir>/domain/$1",
      "^@infrastructures/(.*)$": "<rootDir>/infrastructures/$1",
      "^@shared/(.*)$": "<rootDir>/shared/$1"
    }
  }
}
